{% extends 'base.html.twig' %}

{% block body %}
<div class="container">
    <div class="row">

    {% for joueur in joueurs %}
        <div class="col-md-3">
<div>
  <img src="{{ joueur.strThumb }}" typeof="" width="100" height="75"/>

        <p>{{ joueur.strPlayer | raw }}</p>
